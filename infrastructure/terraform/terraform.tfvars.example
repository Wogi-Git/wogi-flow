# Example Terraform variables
# Copy to terraform.tfvars and customize

aws_region  = "eu-west-1"
environment = "dev"

# Cognito callback URLs (add your app URLs)
cognito_callback_urls = [
  "http://localhost:3000/callback",
  "https://app.wogi-flow.dev/callback"
]

cognito_logout_urls = [
  "http://localhost:3000/logout",
  "https://app.wogi-flow.dev/logout"
]

# API settings
api_stage_name = "v1"

# Lambda settings
lambda_memory_size = 256
lambda_timeout     = 30
